datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Client {
  id   String @id @default(uuid())
  type String

  @@map("clients")
}

model Message {
  messageId  String @id @default(uuid())
  clientId   String
  clientType String
  latitude   String
  longitude  String
  event      String

  @@map("messages")
}
