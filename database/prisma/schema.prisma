datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Client {
  id   String @id @default(uuid())
  type String

  @@map("clients")
}

model Message {
  id        String @id @default(uuid())
  latitude  String
  longitude String
  event     String

  @@map("messages")
}
